digraph {
	fake [style=invisible]
	0 [root=true shape=doublecircle]
	1
	2 [shape=doublecircle]
	3 [shape=doublecircle]
	4
	fake -> 0 [style=bold]
	0 -> 4 [label=a]
	2 -> 2 [label="~a"]
	0 -> 0 [label="~a & ~b"]
	4 -> 2 [label="b & ~a"]
	0 -> 1 [label="b & ~a"]
	3 -> 3 [label="~a & ~b"]
	3 -> 4 [label="a & b"]
	4 -> 1 [label="a & ~b"]
	2 -> 4 [label=a]
	3 -> 2 [label="b & ~a"]
	4 -> 3 [label="~a & ~b"]
	4 -> 4 [label="a & b"]
	1 -> 1 [label=True]
	3 -> 1 [label="a & ~b"]
}

