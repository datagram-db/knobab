digraph {
	fake [style=invisible]
	0 [root=true shape=doublecircle]
	1 [shape=doublecircle]
	2
	3
	fake -> 0 [style=bold]
	1 -> 2 [label=b]
	2 -> 3 [label="b & ~a"]
	2 -> 0 [label="~a & ~b"]
	3 -> 3 [label=True]
	1 -> 1 [label="~b"]
	2 -> 2 [label="a & b"]
	0 -> 3 [label="b & ~a"]
	2 -> 1 [label="a & ~b"]
	0 -> 2 [label="a & b"]
	0 -> 0 [label="~a & ~b"]
	0 -> 1 [label="a & ~b"]
}

