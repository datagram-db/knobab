digraph {
	fake [style=invisible]
	0 [root=true shape=doublecircle]
	1 [shape=doublecircle]
	2
	3
	fake -> 0 [style=bold]
	0 -> 1 [label="~a"]
	1 -> 2 [label=b]
	3 -> 3 [label="a & b"]
	1 -> 1 [label="~a & ~b"]
	3 -> 1 [label="b & ~a"]
	3 -> 2 [label="~b"]
	2 -> 2 [label=True]
	0 -> 3 [label=a]
	1 -> 3 [label="a & ~b"]
}

